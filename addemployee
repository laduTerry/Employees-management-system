    private void JAddActionPerformed(java.awt.event.ActionEvent evt) {                                     
       String ID = JId.getText();
       String Name = JName.getText();
       String phonenumber = JPnumber.getText();
       String email = JEmail.getText();
       String age = JAge.getText();
       String DOB = JDob.getText();
       String gender = JGender.getSelectedItem()+"";
       String maritalstatus = JMstatus.getSelectedItem()+"";
       String department = JDepartment.getSelectedItem()+"";
       String salary = JSalary.getText();
       String join = JJoin.getText();
       String time = JTime.getSelectedItem()+"";
       
       if (ID.equals("")){
           JOptionPane.showMessageDialog(null,"ID is mandatory");
       }
        if (Name.equals("")){
           JOptionPane.showMessageDialog(null,"Name is mandatory");
       }
         if (phonenumber.equals("")){
           JOptionPane.showMessageDialog(null,"Phone Number is mandatory");
       }
          if (email.equals("")){
           JOptionPane.showMessageDialog(null,"Email is mandatory");
       }
           if (age.equals("")){
           JOptionPane.showMessageDialog(null,"Age is mandatory");
       }
            if (DOB.equals("")){
           JOptionPane.showMessageDialog(null,"DOB is mandatory");
       }
             if (gender.equals("")){
           JOptionPane.showMessageDialog(null,"Gender is mandatory");
       }
              if (maritalstatus.equals("")){
           JOptionPane.showMessageDialog(null,"Marital Status is mandatory");
       }
               if (department.equals("")){
           JOptionPane.showMessageDialog(null,"Department is mandatory");
       }
                if (salary.equals("")){
           JOptionPane.showMessageDialog(null,"Salary is mandatory");
       }
                 if (join.equals("")){
           JOptionPane.showMessageDialog(null,"Join Date is mandatory");
       }
        if (time.equals("")){
           JOptionPane.showMessageDialog(null,"Time Frame is mandatory");
       }
        
    }                                    

    private void JExitActionPerformed(java.awt.event.ActionEvent evt) {                                      
       System.exit(0);
    }                                     

